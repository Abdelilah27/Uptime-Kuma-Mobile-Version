<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="10dp"
    app:cardBackgroundColor="@color/grey"
    app:cardCornerRadius="10dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.3" />


        <androidx.cardview.widget.CardView
            android:id="@+id/profile_card_servers"
            android:layout_width="100dp"
            android:layout_height="100dp"
            app:cardBackgroundColor="@color/main_color"
            app:cardCornerRadius="10dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline11"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/all_servers_slug_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/roboto"
                android:text="VP"
                android:textColor="@color/black"
                android:textSize="30sp" />
        </androidx.cardview.widget.CardView>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline11"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/card_view_status"
                android:layout_width="wrap_content"
                android:layout_height="18sp"
                android:layout_alignParentStart="true"
                android:minWidth="65dp"
                android:minHeight="23dp"
                app:cardBackgroundColor="@color/main_color"
                app:cardCornerRadius="25dp">

                <TextView
                    android:id="@+id/percent_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/roboto"
                    android:text="100%"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/dashbord_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone" />

            <TextView
                android:id="@+id/title_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/roboto"
                android:text="VPN IPSEC MOOVCI"
                android:textColor="@color/white"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/id_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/graph_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="4dp"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_1"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_2"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_3"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_4"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView

                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_5"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_15"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_20"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_6"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_8"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_9"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/server_card_graph_12"
                    android:layout_width="8dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:backgroundTint="@color/background_active_item_all_server_fragment"
                    app:cardCornerRadius="30dp" />
            </LinearLayout>


        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>




