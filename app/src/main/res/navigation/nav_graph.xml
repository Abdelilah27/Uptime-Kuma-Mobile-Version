<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/statusFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.uptime.kuma.views.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_bienvenueFragment"
            app:destination="@id/bienvenueFragment" />
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_splashFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/bienvenueFragment"
        android:name="com.uptime.kuma.views.bienvenue.BienvenueFragment"
        android:label="fragment_bienvenue"
        tools:layout="@layout/fragment_bienvenue">
        <action
            android:id="@+id/action_bienvenueFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.uptime.kuma.views.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <action
            android:id="@+id/action_loginFragment_to_allServersFragment"
            app:destination="@id/allServersFragment" />
        <action
            android:id="@+id/action_loginFragment_to_statusFragment"
            app:destination="@id/statusFragment" />
        <action
            android:id="@+id/action_loginFragment_to_parametreFragment"
            app:destination="@id/parametreFragment" />
    </fragment>
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.uptime.kuma.views.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard">
        <action
            android:id="@+id/action_dashboardFragment_to_serverFragment"
            app:destination="@id/serverFragment" />
    </fragment>
    <fragment
        android:id="@+id/serverFragment"
        android:name="com.uptime.kuma.views.ServerFragment"
        android:label="fragment_server"
        tools:layout="@layout/fragment_server" />
    <fragment
        android:id="@+id/allServersFragment"
        android:name="com.uptime.kuma.views.monitorsList.AllServersFragment"
        android:label="fragment_all_servers"
        tools:layout="@layout/fragment_all_servers">
        <action
            android:id="@+id/action_allServersFragment_to_serverFragment"
            app:destination="@id/serverFragment" />
    </fragment>
    <fragment
        android:id="@+id/statusFragment"
        android:name="com.uptime.kuma.views.status.StatusFragment"
        android:label="fragment_status"
        tools:layout="@layout/fragment_status" >
        <action
            android:id="@+id/action_statusFragment_to_itemStatusFragment"
            app:destination="@id/itemStatusFragment" />
        <action
            android:id="@+id/action_statusFragment_to_addStatusFragment"
            app:destination="@id/addStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/parametreFragment"
        android:name="com.uptime.kuma.views.ParametreFragment"
        android:label="fragment_parametre"
        tools:layout="@layout/fragment_parametre">
        <action
            android:id="@+id/action_parametreFragment_to_apparenceReglageFragment"
            app:destination="@id/apparenceReglageFragment" />
        <action
            android:id="@+id/action_parametreFragment_to_notificationsFragment"
            app:destination="@id/notificationsFragment" />
        <action
            android:id="@+id/action_parametreFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>
    <fragment
        android:id="@+id/apparenceReglageFragment"
        android:name="com.uptime.kuma.views.ApparenceReglageFragment"
        android:label="fragment_apparence_reglage"
        tools:layout="@layout/fragment_apparence_reglage" />
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.uptime.kuma.views.NotificationsFragment"
        android:label="fragment_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.uptime.kuma.views.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/itemStatusFragment"
        android:name="com.uptime.kuma.views.ItemStatusFragment"
        android:label="fragment_item_status"
        tools:layout="@layout/fragment_item_status" />
    <fragment
        android:id="@+id/addStatusFragment"
        android:name="com.uptime.kuma.views.addStatus.AddStatusFragment"
        android:label="fragment_add_status"
        tools:layout="@layout/fragment_add_status" />
</navigation>